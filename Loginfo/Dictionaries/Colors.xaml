<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--SearchBar-->
    <SolidColorBrush x:Key="SearchBoxBorder" Color="#939394"/>

    <!--App Accent Dark/Sidebar-->
    <SolidColorBrush x:Key="SideBar" Color="#98ABFF"/>
    <!--Task List BG-->
    <SolidColorBrush x:Key="TaskListBG" Color="#D9D9D9"/>
    <!--Task View BG-->
    <SolidColorBrush x:Key="TaskViewBG" Color="#FFFFFF"/>
    <!--Task Status BG-->
    <SolidColorBrush x:Key="TaskStatusBG" Color="#BAC8FF"/>
    <!--Default Text Color-->
    <SolidColorBrush x:Key="DefaultText" Color="#141522"/>

    <!--Due date-->
    <SolidColorBrush x:Key="DueDate" Color="#4340DA"/>

    <!--Task view button-->
    <SolidColorBrush x:Key="Button.Complete" Color="#8E92BC"/>
    <SolidColorBrush x:Key="Button.CompleteOver" Color="#6F74A4"/>
    <SolidColorBrush x:Key="Button.CompleteClick" Color="#BDC3FA"/>
    <SolidColorBrush x:Key="Button.Edit" Color="#666666"/>
    <SolidColorBrush x:Key="Button.EditOver" Color="#414141"/>
    <SolidColorBrush x:Key="Button.EditClick" Color="#BDBDBD"/>
    <SolidColorBrush x:Key="Button.Delete" Color="#490206"/>
    <SolidColorBrush x:Key="Button.DeleteOver" Color="#2D0104"/>
    <SolidColorBrush x:Key="Button.DeleteClick" Color="#A70811"/>

    <!--Importance Color-->
    <SolidColorBrush x:Key="Importance-Low" Color="#34EB07"/>
    <SolidColorBrush x:Key="Importance-Med" Color="#FABB18"/>
    <SolidColorBrush x:Key="Importance-High" Color="#FF7008"/>
    <SolidColorBrush x:Key="Importance-Crit" Color="#FF0206"/>

    <!--Task Progress-->
    <SolidColorBrush x:Key="Progress-InProgress" Color="#518657"/>
    <SolidColorBrush x:Key="Progress-Completed" Color="#8E92BC"/>
    <SolidColorBrush x:Key="Progress-NotStarted" Color="#b1b1b1"/>
    <SolidColorBrush x:Key="Progress-Late" Color="#b46161"/>
    <SolidColorBrush x:Key="Progress-Archived" Color="#c8a36c"/>
    <SolidColorBrush x:Key="Progress-Deleted" Color="#FF606060"/>

</ResourceDictionary>